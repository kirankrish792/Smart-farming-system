              <span class="menu-icon">
                <i class="mdi mdi-home"></i>
              </span>
              <span class="menu-title">Home</span>
            </a>
          </li>
          <hr>
          <li class="nav-item nav-category">
            <span class="nav-link">Government Alerts</span>
          </li>
          <li class="nav-item menu-items">
            <a class="nav-link" href="https://keralaagriculture.gov.in/en/krishi-jaalakam/">
              <span class="menu-icon">
                <i class="mdi mdi-redo-variant"></i>
              </span>
              <span class="menu-title">Kerala-Government</span>
            </a>
          </li>
          <li class="nav-item menu-items">
            <a class="nav-link" href="https://agricoop.nic.in/">
              <span class="menu-icon">
                <i class="mdi mdi-redo-variant"></i>
              </span>
              <span class="menu-title">Central-Government</span>
            </a>
          </li>
          <hr>
          <li class="nav-item nav-category">
            <span class="nav-link">Complete Data</span>
          </li>
          <li class="nav-item menu-items">
            <a class="nav-link" href="/monitor/temp">
              <span class="menu-icon">
                <i class="mdi mdi-temperature-celsius"></i>
              </span>
              <span class="menu-title">Temperature-data</span>
            </a>
          </li>
          <li class="nav-item menu-items">
            <a class="nav-link" href="/monitor/humidity">
              <span class="menu-icon">
                <i class="mdi mdi-cloud-outline"></i>
              </span>
              <span class="menu-title">Humidity-data</span>
            </a>
          </li>
          <li class="nav-item menu-items">
            <a class="nav-link" href="/monitor/moisture">
              <span class="menu-icon">
                <i class="mdi mdi-weather-hail"></i>
              </span>
              <span class="menu-title">Moisture-data</span>
            </a>
          </li>
        </ul>
      </nav>
      <!-- partial -->
      <div class="container-fluid page-body-wrapper">
        <!-- partial:partials/_navbar.html -->
        <nav class="navbar p-0 fixed-top d-flex flex-row">
          <div class="navbar-brand-wrapper d-flex d-lg-none align-items-center justify-content-center">
            <h4>Smart Farming</h4>
          </div>
          <div class="navbar-menu-wrapper flex-grow d-flex align-items-stretch">
            <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
              <span class="mdi mdi-menu"></span>
            </button>
            <ul class="navbar-nav w-100">
              <li class="nav-item w-100">
              </li>
            </ul>
            <ul class="navbar-nav navbar-nav-right">
              <li class="nav-item dropdown border-left">
                <a class="nav-link count-indicator dropdown-toggle" id="messageDropdown" href="#" data-bs-toggle="dropdown" aria-expanded="false">
                  <i class="mdi mdi-email"></i>
                  <span class="count bg-success"></span>
                </a>
                <div class="dropdown-menu dropdown-menu-right navbar-dropdown preview-list" aria-labelledby="messageDropdown">
                  <h6 class="p-3 mb-0">Messages</h6>
                  <h4 class="text-center">Coming Soon</h4>
                </div>
              </li>
              <li class="nav-item dropdown border-left">
                <a class="nav-link count-indicator dropdown-toggle" id="notificationDropdown" href="#" data-bs-toggle="dropdown">
                  <i class="mdi mdi-bell"></i>
                  <span class="count bg-danger"></span>
                </a>
                <div class="dropdown-menu dropdown-menu-right navbar-dropdown preview-list" aria-labelledby="notificationDropdown">
                  <h6 class="p-3 mb-0">Notifications</h6>
                  <div class="dropdown-divider"></div>
                  <a class="dropdown-item preview-item">
                    <div class="preview-thumbnail">
                      <div class="preview-icon bg-dark rounded-circle">
                        <i class="mdi mdi-calendar text-success"></i>
                      </div>
                    </div>
                    <div class="preview-item-content">
                      <!-- <p class="preview-subject mb-1">Event today</p>
                      <p class="text-muted ellipsis mb-0"> Just a reminder that you have an event today </p> -->
                    </div>
                  </a>
              </li>
              
            </ul>
            <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
              <span class="mdi mdi-format-line-spacing"></span>
            </button>
          </div>
        </nav>
        <!-- partial -->
        <div class="main-panel">
          <div class="content-wrapper">
            <div class="row">
              <div class="col-xl-3 col-sm-6 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <div class="row">
                      <div class="col-9">
                        <div class="d-flex align-items-center align-self-start">
                          <h3 class="mb-0"><%= humidityValue[0].value %></h3>
                          <p class="text-<%= humidityValue[0].value-humidityValue[1].value < 0 ? "danger" : "success" %> ms-2 mb-0 font-weight-medium"><%= humidityValue[0].value-humidityValue[1].value %></p>
                        </div>
                      </div>
                      <div class="col-3">
                         <% if(humidityValue[0].value-humidityValue[1].value < 0){  %>
                        <div class="icon icon-box-danger">
                          <span class="mdi mdi-arrow-bottom-left icon-item"></span>
                        </div>
                        <% } else { %>
                          <div class="icon icon-box-success">
                          <span class="mdi mdi-arrow-top-right icon-item"></span>
                        </div>
                          <% } %>  
                      </div>
                    </div>
                    <h6 class="text-muted font-weight-normal">Humidity</h6>
                  </div>
                </div>
              </div>
              <div class="col-xl-3 col-sm-6 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <div class="row">
                      <div class="col-9">
                        <div class="d-flex align-items-center align-self-start">
                          <h3 class="mb-0"><%= tempValue[0].value %></h3>
                          <p class="text-<%= tempValue[0].value-tempValue[1].value < 0 ? "danger" : "success" %> ms-2 mb-0 font-weight-medium"><%= tempValue[0].value-tempValue[1].value %></p>
                        </div>
                      </div>
                      <div class="col-3">
                         <% if(tempValue[0].value-tempValue[1].value < 0){  %>
                        <div class="icon icon-box-danger">
                          <span class="mdi mdi-arrow-bottom-left icon-item"></span>
                        </div>
                        <% } else { %>
                          <div class="icon icon-box-success">
                          <span class="mdi mdi-arrow-top-right icon-item"></span>
                        </div>
                          <% } %>  
                      </div>
                    </div>
                    <h6 class="text-muted font-weight-normal">Temperature</h6>
                  </div>
                </div>
              </div>
              <div class="col-xl-3 col-sm-6 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <div class="row">
                      <div class="col-9">
                        <div class="d-flex align-items-center align-self-start">
                          <h3 class="mb-0"><%= moistureValue[0].value %> </h3>
                          <p class="text-<%= moistureValue[0].value-moistureValue[1].value < 0 ? "danger" : "success" %>  ms-2 mb-0 font-weight-medium"><%= moistureValue[0].value-moistureValue[1].value %></p>
                        </div>
                      </div> 
                      <div class="col-3">
                        <% if(moistureValue[0].value-moistureValue[1].value < 0){  %>
                        <div class="icon icon-box-danger">
                          <span class="mdi mdi-arrow-bottom-left icon-item"></span>
                        </div>
                        <% } else { %>
                          <div class="icon icon-box-success">
                          <span class="mdi mdi-arrow-top-right icon-item"></span>
                        </div>
                          <% } %>  
                      </div>
                    </div>
                    <h6 class="text-muted font-weight-normal">Moisture</h6>
                  </div>
                </div>
              </div>
              <div class="col-xl-3 col-sm-6 grid-margin stretch-card">
                <div class="card">
                  <div class="card-body">
                    <div class="row">
                      <div class="col-6">
                        <div class="d-flex align-items-center align-self-start">
                          <h3 class="mb-0"><%= weDa.main %> </h3>
                        </div>
                      </div>
                      <div class="col-6">
                          <p class="text-success ms-2 mb-0 font-weight-medium"><%= weDa.description %> </p>
  
                      </div>
                    </div>
                    <h6 class="text-muted font-weight-normal">Weather</h6>
                  </div>
                </div>
              </div>
            </div>
            <div class="row">
               <canvas id="myChart" style="width:100%;"></canvas>
            </div>
            
            
                  </div>
                </div>
              </div>
            </div>
          <!-- content-wrapper ends -->
          <!-- partial:partials/_footer.html -->
          <!-- partial -->
        </div>
        <!-- main-panel ends -->
      </div>
      <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="assets/vendors/js/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page -->
    <script>
 var xValues = [<%= finalTime %>];

new Chart("myChart", {
  type: "line",
  data: {
    labels: xValues,
    datasets: [{
      label: 'Humidity',
      data: [<%= humdata %>],
      borderColor: "red",
      fill: false,
    },{
      label: 'Temperature',
      data: [<%= temdata %>],
      borderColor: "green",
      fill: false
    },{
      label:'Moisture',
      data: [<%= moisdata %>],
      borderColor: "blue",
      fill: false
    }]
  },
  options: {
    legend: {display: true},
  }
});
</script>
    <script src="assets/vendors/chart.js/Chart.min.js"></script>
    <script src="assets/vendors/progressbar.js/progressbar.min.js"></script>
    <script src="assets/vendors/jvectormap/jquery-jvectormap.min.js"></script>
    <script src="assets/vendors/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="assets/vendors/owl-carousel-2/owl.carousel.min.js"></script>
    <script src="assets/js/jquery.cookie.js" type="text/javascript"></script>
    <!-- End plugin js for this page -->
    <!-- inject:js -->
    <script src="assets/js/off-canvas.js"></script>
    <script src="assets/js/hoverable-collapse.js"></script>
